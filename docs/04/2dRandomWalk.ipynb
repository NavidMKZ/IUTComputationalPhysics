    "ax3.axvline(np.mean(all_y), color='red', linestyle='--', label=f\"Mean = {np.mean(all_y):.2f}\")\n",
    "ax3.set_title(\"Y Distribution\")\n",
    "ax3.set_xlabel(\"Y Values\")\n",
