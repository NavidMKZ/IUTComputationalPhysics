    "def decay(N0 , lambda0=0.1 ):\n",
    "    steps , ns = [] , []\n",
    "    i = 0\n",
    "    while N0 > 0:\n",
    "        steps.append(i)\n",
    "        ns.append(N0)\n",
    "        N0 -= np.sum( np.random.uniform(0,1, N0) < lambda0 )\n",
    "        i += 1\n",
    "\n",
    "    return steps , ns \n",
